<%-include("./partials/header") -%>


<form action="/blogs/compose" method="post">

 <div class="form-group">
    <label for="blogtitle">Title</label>
    <input type="text" class="form-control" id="blogtitle" placeholder="Enter Title" name="title" required>
  </div>

<div class="form-group">
    <label for="blogcontent">Example textarea</label>
    <textarea class="form-control" id="blogcontent" rows="3" name="content" required></textarea>
    <button type="submit" class="btn btn-primary" name="submit" style="margin-top:5px">Publish</button>
  </div>
</form>
</div>

<%-include("./partials/footer") -%>